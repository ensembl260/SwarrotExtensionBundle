{
    "name": "ensembl260/swarrot-extension-bundle",
    "description": "SwarrotBundle extension",
    "type": "symfony-bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Ensembl260\\SwarrotExtensionBundle\\": ""
        }
    },
    "require": {
        "php": "8.*",
        "ext-json": "*",
        "ensembl260/rmq-toolkit": "v1.0.0",
        "swarrot/swarrot-bundle": "2.6.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/event-dispatcher": "7.3.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:ensembl260/rmq-toolkit.git",
            "no-api": true
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "php-amqplib/php-amqplib": "^3.2",
        "slevomat/coding-standard": "8.24.*",
        "phpmd/phpmd": "^2.9",
        "squizlabs/php_codesniffer": "4.*",
        "symfony/error-handler": "7.3.*",
        "phpspec/prophecy": "^1.15"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
