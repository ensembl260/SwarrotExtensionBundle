{
    "name": "ma-residence/swarrot-extension-bundle",
    "description": "SwarrotBundle extension",
    "type": "symfony-bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "MR\\SwarrotExtensionBundle\\": ""
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "psr/log": "~1.0",
        "swarrot/swarrot": "^4.0",
        "swarrot/swarrot-bundle": "^2.1",
        "symfony/framework-bundle": "6.0.*",
        "symfony/validator": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/event-dispatcher": "5.4.*",
        "odolbeau/rabbit-mq-admin-toolkit": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "php-amqplib/php-amqplib": "^3.2",
        "slevomat/coding-standard": "^6.4",
        "phpmd/phpmd": "^2.9",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/error-handler": "5.4.*",
        "phpspec/prophecy": "^1.15"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
